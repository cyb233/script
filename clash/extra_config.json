    "DOMAIN-SUFFIX,gamer.com.tw,TW",
    "DOMAIN,checkip.amazonaws.com,DIRECT",
    "DOMAIN-SUFFIX,playnite.link,Proxies",
    "DOMAIN-SUFFIX,itch.io,Proxies",
